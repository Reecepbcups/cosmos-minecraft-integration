{
	"name": "svelte-with-tail",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev --host --port 6010",
		"build": "vite build --host --port 6010",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
